{
  "name": "BORPTS",
  "description" : "BackEnd PenTester",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "hugme.png"
  },

  "manifest_version": 2,
  "permissions": ["tabs", "activeTab"],

  "content_scripts": [
    {
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": ["contentscript.js"],
    "run_at": "document_end"
    }
  ],

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens flkdjsdfk"
    }
  }



}